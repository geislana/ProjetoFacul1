datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL") // Conex√£o com o MySQL
}

generator client {
  provider = "prisma-client-js"
}

model Animal {
  id       Int      @id @default(autoincrement())
  nome     String
  idade    Int
  especie  String
  criadoEm DateTime @default(now())
}
